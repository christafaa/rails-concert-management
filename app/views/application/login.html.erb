<div class="center">
  <h5>Login</h5>

  <%= form_tag "/sessions" do %>
    <%= label_tag :username, "Username:" %>
    <%= text_field_tag :username %><br><br>
    <%= label_tag :password, "Password:" %>
    <%= password_field_tag :password %><br><br>
    <%= submit_tag "Login" %>
  <% end %>

  <h5><%= link_to "Login with GitHub", "/auth/github" %></h5>

  <h5><%= link_to "Create an Account", new_user_path %></h5>
</div>
